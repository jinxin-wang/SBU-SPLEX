loadData <- function(fname="obeses_temoins.txt"){
    data.mtx <- t(read.delim(fname,header=T))
    data.colnames <- colnames(data.mtx)
    data.obeseLabs <- data.colnames[1:25]
    data.temoinLabs <- data.colnames[26:length(data.colnames)]
    # data.classID <- c(rep(1,25),rep(2,length(data.colnames)-25))
    return(data)
}
